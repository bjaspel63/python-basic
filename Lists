# Create a list
fruits = ['apple', 'banana', 'cherry']

# Access elements
print(fruits[0]) # Output: apple

# Modify elements
fruits[1] = 'blueberry'
print(fruits) # Output: ['apple', 'blueberry', 'cherry']

# Add and remove elements
fruits.append('orange')
fruits.remove('apple')
print(fruits) # Output: ['blueberry', 'cherry', 'orange']
